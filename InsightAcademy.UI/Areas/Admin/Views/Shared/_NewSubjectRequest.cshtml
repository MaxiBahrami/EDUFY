@using InsightAcademy.DomainLayer.Dtos
@using InsightAcademy.UI.Helper
@model IEnumerable<NewSubjectRequestDto>
<div class="col-lg-12 col-md-12 col-sm-12 col-12">
    <div class="card-box">
        <div class="card-head">
            <header>New Subject Request</header>
        </div>
        <div class="card-body ">
            <div class="table-responsive">
                <table class="table table-hover dashboard-task-infos">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Subject</th>
                            <th>Category</th>

                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var teachers in Model)
                        {
                            <tr>
                                <td>
                                    1
                                </td>
                                <td>@teachers.Subject</td>

                                <td>@teachers.Category</td>
                                <td>
                                    <a href="javascript:void(0)" onclick="" class="tblEditBtn">
                                        <i class="fa fa-pencil"></i>
                                    </a>

                                </td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

@section Scripts
{
    <script>
        function ApproveSubject(subjectId) { 
            alert('ok')
        }
    </script>
}
